<% content_for :page_specific_javascript do -%>
  <%= javascript_include_tag 'schedule' %>
<% end -%>

<%= javascript_tag "var sessionIdList = [#{attendee_session_ids.join(',')}];" %>

<div class="row_top_tile_sub">
  <div class="container_12">
    <div class="grid_8">
      <div class="page_title">
        <%= t('.schedule') %>
      </div>
    </div>
  </div>
</div>

<% if Refinery::Setting.find_or_set(:hide_schedule_details, 'false') == true %>
  <div id="main" class="shape">
    <div class="container_12 pad3">
      <p><%= raw t('.hide_schedule_details') %></p>
    </div>
  </div>
<% else %>
  <%= render 'cached_schedule', :cache_key => schedule_key %>
<% end %>
