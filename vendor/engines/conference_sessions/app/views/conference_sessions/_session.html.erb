<div class="grid_3 speaker">
  <%= link_to image_tag_for(speaker.image, :small), session, :class => 'pic_speaker', :rel => ' ' %>
  <div style="float: left;">
    <h4><%= link_to talk.title, session %></h4>
    <h5><%= link_to speaker_for(talk), speaker.company_url %></h5>
    <%= twitter_link speaker.twitter_id, :span %>
  </div>
</div>
