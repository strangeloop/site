<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(sponsorship_level) -%>">
  <span class='title'>
    <%= sponsorship_level.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_sponsorship_level_path(sponsorship_level),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_sponsorship_level_path(sponsorship_level),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => sponsorship_level.title),
        :method => :delete %>
  </span>
</li>

